if not getgenv then
    function getgenv() return _G end
end
local UniverseId = game.GameId
local GameName = ""
if UniverseId == 7326934954 then
    GameName = "99nights"
elseif UniverseId == 9051406594 then
    GameName = "duellinggrounds"
end
if GameName then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/dunnook/RiceHub/refs/heads/main/"..GameName))()
else
    print("Game is not supported by rice hub")
end
